{
  "name": "a2g",
  "version": "0.1.5",
  "description": "an extremely simple Angular 2 source code generator",
  "main": "index.ts",
  "bin": {
    "a2g": "bin/a2g.js"
  },
  "keywords": [
    "angular2",
    "generator"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/brakmic/a2g"
  },
  "scripts": {
    "rimraf": "rimraf",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "webpack": "webpack",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist",
    "clean:dist": "npm run rimraf -- dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch:prod": "npm run build:prod -- --watch",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "webpack --config config/webpack.prod.js  --progress --profile --bail",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "test": "rimraf ./spec/test_out && jasmine-ts ./spec/**/*.spec.ts",
    "start": "ts-node index.ts",
    "preversion": "npm test",
    "version": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "author": "Harris Brakmic",
  "license": "MIT",
  "dependencies": {
    "bows": "^1.6.0",
    "rxjs": "^5.4.0",
    "typedoc": "^0.7.0",
    "typescript-collections": "^1.2.3"
  },
  "devDependencies": {
    "@angular/core": "^4.1.3",
    "@types/es6-shim": "^0.31.33",
    "@types/jasmine": "^2.5.47",
    "@types/lodash": "^4.14.63",
    "@types/node": "^7.0.18",
    "@types/requirejs": "^2.1.29",
    "@types/source-map": "^0.5.0",
    "@types/uglify-js": "^2.6.28",
    "@types/webpack": "^2.2.15",
    "awesome-typescript-loader": "^3.1.3",
    "cardinal": "^1.0.0",
    "chai": "^3.5.0",
    "circular-json": "^0.3.1",
    "codelyzer": "^3.0.1",
    "compression-webpack-plugin": "^0.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.1",
    "diff": "^3.2.0",
    "ecstatic": "^2.1.0",
    "es6-module-loader": "^0.17.11",
    "es6-promise": "^4.1.0",
    "es6-promise-loader": "^1.0.2",
    "es7-reflect-metadata": "^1.6.0",
    "exports-loader": "^0.6.4",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "font-awesome-sass-loader": "^2.0.1",
    "imports-loader": "^0.7.1",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine": "^2.6.0",
    "jasmine-core": "^2.6.2",
    "jasmine-node": "^1.14.5",
    "jasmine-spec-reporter": "^4.1.0",
    "jasmine-ts": "^0.1.2",
    "jquery": "^2.2.3",
    "js-base64": "^2.1.9",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.4",
    "mocha": "^3.4.1",
    "nconf": "^0.8.4",
    "node-sass": "^4.5.3",
    "parse5": "^3.0.2",
    "phantomjs": "^2.1.7",
    "polyfiller": "0.0.32",
    "polyfiller-catalog": "0.0.31",
    "raw-loader": "0.5.1",
    "readline2": "^1.0.1",
    "redeyed": "^1.0.1",
    "remap-istanbul": "^0.9.5",
    "resolve-url-loader": "^2.0.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "script-loader": "^0.7.0",
    "source-map-loader": "^0.2.1",
    "steal": "^1.5.2",
    "string-loader": "^0.0.1",
    "string-replace-loader": "^1.2.0",
    "style-loader": "^0.17.0",
    "to-string-loader": "^1.1.5",
    "ts-helpers": "^1.1.2",
    "ts-loader": "^2.0.3",
    "ts-node": "^3.0.4",
    "tslint": "^5.2.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.3.2",
    "typings": "^2.1.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^4.1.0"
  }
}
